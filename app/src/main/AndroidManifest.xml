<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yc.bookmark">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.MyBookmarkAddOrEditeActivity" />
        <activity android:name=".ui.MyBookMarkActivity" />
        <activity android:name=".ui.MyBookmarkAddEditFolderActivity" />
        <activity android:name=".ui.MyBookmarkChildFolderActivity"></activity>
        <activity android:name=".ui.MyBookMarkTreeViewActivity"></activity>
    </application>

</manifest>